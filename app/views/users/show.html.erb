<h1>@user.name's page</h1>
<%= if logged_in %>
    <p>Name: <%= @user.name %></p>
    <p>Tickets: <%= @user.tickets%></p>
    <p>Mood: <%= @user.mood %></p>
    <p>Height: <%= @user.height %></p>
    <%= link_to("See All Attractions"), attractions_path %>
<% else %>
    <%#redirect_to root_path %>
<% end %>